<?xml version="1.0"?>
<project name="LogcatFileViewer" basedir=".">

	<property name="lib.dir" value="./libs" />

	
	<taskdef name="swtjar" classname="org.swtjar.ant.SWTJarTask" classpath="${lib.dir}/swt/swtjar.jar" />

	<path id="classpath">
		<fileset dir="${lib.dir}" includes="**/*.jar" />
	</path>


	<target name="compile">
		<mkdir dir="${classes.dir}" />
		<javac srcdir="${src.dir}" destdir="${classes.dir}" classpathref="classpath" />
	</target>
</project>